@model IEnumerable<FirstWebApp.Models.Employee>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>
<table>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Age</th>
    </tr>
@foreach (var item in ViewBag.Employees)
{
    <tr>
        <td>@item.Id</td>
        <td>@item.Name</td>
         <td>@item.Age</td>
    </tr>
}
</table>
<hr />
<h2>@Html.ActionLink("Add New Employee","Create","Employee")</h2>
<table border="1">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Age</th>
    </tr>
@foreach (var item in Model)
{
    <tr>
        <td>@item.Id</td>
      @*  <td>@item.Name</td>
        <td>@item.Age</td>
        <td><a href="Employee/Details/"+@item.Id>Details</a></td>*@
         <td>@Html.ActionLink("Show Details","Details","Employee",new{id=@item.Id})</td>|
         <td>@Html.ActionLink("Update","Edit","Employee",new{id=@item.Id})</td>|
          <td>@Html.ActionLink("Delete","Delete","Employee",new{id=@item.Id})</td>
    </tr>
}
</table>

